<Window x:Name="ExpertSystemMainWindow" x:Class="ExpertSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExpertSystem"
        mc:Ignorable="d"
        Title="MainWindow" Height="947" Width="1722" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="802*"/>
            <RowDefinition Height="129*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="563*"/>
            <ColumnDefinition Width="593*"/>
            <ColumnDefinition Width="566*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="StartConsultationButton" Content="Запустить консультацию" HorizontalAlignment="Left" Margin="177,4,0,0" VerticalAlignment="Top" Height="38" Width="210" RenderTransformOrigin="0.171,0.58" Grid.Column="1" Grid.Row="1"/>
        <Button x:Name="ShowLogicResultButton" Content="Посмотреть пояснение результата" HorizontalAlignment="Left" Margin="177,0,0,0" VerticalAlignment="Center" Height="40" Width="210" Grid.Column="1" Grid.Row="1"/>
        <Label x:Name="DomainsLabel" Content="&#x9;           Домены" Margin="40,44,0,0" Height="31" Width="232" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label x:Name="VariablesLabel" Content="&#x9;          Значения" HorizontalAlignment="Left" Margin="312,44,0,0" VerticalAlignment="Top" Height="31" Width="232"/>
        <ListBox x:Name="DomainsListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="40,75,291,533"/>
        <ListBox x:Name="VariablesListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="312,75,19,533"/>
        <Button x:Name="AddDomainButton" Content="Добавить" HorizontalAlignment="Left" Margin="40,274,0,0" VerticalAlignment="Top" Width="73" Height="20"/>
        <Button x:Name="ChangeDomainButton" Content="Изменить" HorizontalAlignment="Left" Margin="120,274,0,0" VerticalAlignment="Top" Width="72" Height="20"/>
        <Button x:Name="DeleteDomainButton" Content="Удалить" HorizontalAlignment="Left" Margin="199,274,0,0" VerticalAlignment="Top" Width="73" Height="20"/>
        <Button x:Name="AddVariableButton" Content="Добавить" HorizontalAlignment="Left" Margin="312,274,0,0" VerticalAlignment="Top" Width="73" Height="20"/>
        <Button x:Name="ChangeVariableButton" Content="Изменить" HorizontalAlignment="Left" Margin="392,274,0,0" VerticalAlignment="Top" Width="72" Height="20"/>
        <Button x:Name="DeleteVariableButton" Content="Удалить" HorizontalAlignment="Left" Margin="471,274,0,0" VerticalAlignment="Top" Width="73" Height="20"/>
        <Label x:Name="DomainVariablesLabel" Content="ДОМЕННЫЕ ЗНАЧЕНИЯ" HorizontalAlignment="Center" Height="48" Width="369" VerticalAlignment="Top" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold"/>
    </Grid>
</Window>
